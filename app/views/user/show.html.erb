<% if current_user && current_user.id.to_i == params[:id].to_i%>
  <h2>Hello <%= current_user.email %></h2>
  <p><%= link_to "Account Settings", edit_user_registration_path, class: 'btn btn-info' %></>
  <h3>All wikis created:</h3>
  <%= render partial: 'wikis/wiki', locals: {wikis: @wikis} %>

<% end %>